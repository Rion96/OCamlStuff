LET N 3
LET X ARR N

FOR LET I 0; I < N; I++
    LET X[I] ARR N
    FOR LET J 0; J < N; J++
        LET X[I][J] ARR N
        FOR LET K 0; K < N; K++
            LET X[I][J][K] I + J + K
            PRINTLN X[I][J][K]
        ENDFOR
    ENDFOR
ENDFOR

LET X[0][1][2] "TEST"
PRINTLN X[0][1][2][1] # Should print E