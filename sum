# Sum an array of numbers (for interpreter.ml)

FUN MAIN
    PRINT "How many elements? "
    SCAN INT N
    LET X ARR N
    FOR LET I 1; I <= N; I++
        PRINT "Enter number " + I + ": "
        SCAN INT X[I - 1]
    ENDFOR
    PRINTLN "Sum is: " + SUM X
ENDFUN

# A: Array
FUN SUM A
    LET ACC 0
    LET N LEN A
    FOR LET I 0; I < N; I++
        LET ACC (ACC + A[I])
    ENDFOR
    RETURN ACC
ENDFUN

MAIN