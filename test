# Test script for interpreter.ml

LET V 5 # This is a comment!
LET W "World!"
# This is also a comment!
PRINTLN 123
PRINTLN V + 5 - 1
PRINTLN 1 + 3 - 1 + V + 1
PRINTLN 1 + (-2)
PRINTLN 2 * (1 + 2)
LET X (-2) * (-1)
LET Y 14
PRINTLN Y / X
PRINTLN "Hello " + W
PRINTLN 5 % 2
PRINTLN X < Y
PRINTLN !(TRUE) || !(43 < 2)
LET T TRUE
PRINTLN !T
IF 1 + 1 = 2
  PRINTLN "Duh!"
  LET ANS 1 + 1
  PRINTLN "1 + 1 is:"
  PRINTLN ANS
  IF TRUE
    PRINTLN "Yup!"
  ELSE
    PRINTLN "LOL"
  ENDIF
ELSE
  PRINTLN "Wait... what?"
  IF TRUE
    PRINTLN "Bla"
  ENDIF
ENDIF

LET X 1
WHILE X <= 10
  PRINTLN "Looping..."
  PRINTLN X
  X++
ENDWHILE
PRINTLN X
LET TEST ARR 16
LET TEST[X - 1 * 2] 123
PRINTLN TEST[3 * 3]
LET F 4.3; PRINTLN F
LET F 4. ; PRINTLN F
PRINTLN 3.5 + 2 / 3.
PRINTLN 5 ^ (2 - 1) * 2 + 5

FUN ADD X Y
  RETURN X + Y
ENDFUN

PRINTLN ADD 5 7
PRINTLN '9' - '0'