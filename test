# Test script for interpreter.ml

LET V 5 # This is a comment!
LET W "World!"
# This is also a comment!
PRINT 123
PRINT V + 5 - 1
PRINT 1 + 3 - 1 + V + 1
PRINT 1 + (-2)
PRINT 2 * (1 + 2)
LET X (-2) * (-1)
LET Y 14
PRINT Y / X
PRINT "Hello " + W
PRINT 5 % 2
PRINT X < Y
PRINT !(TRUE) || !(43 < 2)
LET T TRUE
PRINT !T
IF 1 + 1 = 2
  PRINT "Duh!"
  LET ANS 1 + 1
  PRINT "1 + 1 is:"
  PRINT ANS
  IF TRUE
    PRINT "Yup!"
  ELSE
    PRINT "LOL"
  ENDIF
ELSE
  PRINT "Wait... what?"
  IF TRUE
    PRINT "Bla"
  ENDIF
ENDIF

LET X 1
WHILE X <= 10
  PRINT "Looping..."
  PRINT X
  LET X X + 1
ENDWHILE